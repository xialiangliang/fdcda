###set($layout = "/layout/layout2.vm")
#set($pageTitle = "title")
#set($JS = ["/js/jquery.form.min.js", "/js/page/sysOutlets/new-update.js"])
<div class="yw_buyerContainer">
<div class="yw_breadcrumb">
    当前位置：<a href="javascript:;">$!{currentPosStr}</a>
</div>
<div class="col-sm-12">
    <form action="/sysOutlets/update"  method="post" class="form-horizontal layui-form" id="updateForm_sysOutlets">
        <input type="hidden" name="id" value="$!{param.id}" />
        <div class="form-group">
            <label for="userId" class="col-sm-2 control-label">用户id</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="userId" name="userId" value="$!{param.userId}" >
            </div>
        </div>    
        <div class="form-group">
            <label for="name" class="col-sm-2 control-label">门店名</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="name" name="name" value="$!{param.name}" >
            </div>
        </div>    
        <div class="form-group">
            <label for="address" class="col-sm-2 control-label">地址</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="address" name="address" value="$!{param.address}" >
            </div>
        </div>      
        <div class="form-group" align="right">
            <div class="col-sm-12">
                <button type="button" class="btn btn-default btn-close">关闭</button>
                <button type="submit" class="btn btn-primary">提交</button>
            </div>
        </div>
    </form>
</div>
</div>