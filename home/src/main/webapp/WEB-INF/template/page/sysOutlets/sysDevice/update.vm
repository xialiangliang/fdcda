#set($layout = "/layout/layout2.vm")
#set($pageTitle = "title")
#set($JS = ["/js/jquery.form.min.js", "/js/page/sysOutlets/sysDevice/new-update.js"])
<div class="col-sm-12">
    <form action="/sysOutlets/sysDevice/update"  method="post" class="form-horizontal" id="updateForm_sysDevice">
        <input type="hidden" name="id" value="$!{param.id}" />
        <div class="form-group">
            <label for="seqno" class="col-sm-2 control-label">设备序列号</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="seqno" name="seqno" value="$!{param.seqno}" >
            </div>
        </div>     
        <div class="form-group" align="right">
            <div class="col-sm-12">
                <button type="button" class="btn btn-default btn-close">关闭</button>
                <button type="submit" class="btn btn-primary">提交</button>
            </div>
        </div>
    </form>
</div>