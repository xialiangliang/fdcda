#set($JS = ["/js/page/sysUser/list.js"])
<div class="main-content">
    <div class="yw_breadcrumb">
        当前位置：<a href="javascript:;">$!{currentPosStr}</a>
    </div>
	<div class="page-content">
        <div class="sc-where-bar">
            <div class="col-sm-11">
                <form class="form-inline" role="form">
                    <div class="form-group">
                        <input type="text" class="form-control" name="keyword" value="$!{query.keyword}"
						placeholder="关键字">
                    </div>
                    <div class="form-group">
                        <div class="col-sm-12">
                            <button class="btn btn-primary btn-search" type="submit">查询</button>
                        </div>
                    </div>
                </form>
            </div>
            <div class="col-sm-1 text-right">
                <a href="#" class="btn btn-success btn-add j_new-btn">添加</a>
            </div>
        </div>
        <div id="newUpdateDialogFrame" class="tipDialogFrame"></div>
        <div id="tipDialogFrame" class="tipDialogFrame"></div>

	        <table class="table table-striped table-bordered table-hover dataTable">
	            <thead>
	            <tr>
	                <th>ID</th>
	                <th>登录名</th>
	                <th>姓名</th>
	                <th>手机号</th>
	                <th>固定电话</th>
	                <th>邮件地址</th>
	                <th>地址</th>
	                <th>状态</th>
	                <th>创建时间</th>
	             
	                <th class="action-cl">操作</th>
	               
	            </tr>
	            </thead>
	            <tbody>
	                #foreach($row in $result.rows)
	                <tr>
	                	<td> $!{row.id} </td>
	                	<td> $!{row.loginname} </td>
	                	<td> $!{row.username} </td>
	                	<td> $!{row.phone} </td>
	                	<td> $!{row.telephone} </td>
	                	<td> $!{row.email} </td>
	                	<td> $!{row.address} </td>
	                	<td> #if ($!{row.valid} == 1) 有效 #else 无效 #end </td>
	                	<td> $!{row.createTimeStr} </td>
		                <td class="action-cl">
	                    	<a href="#" class="j_update-btn" data-id="$!{row.id}">修改</a>
	                    </td>
	                </tr>
	                #end
	            </tbody>
	       	</table>
	        <div class="dataTables_wrapper f-mt2">
	            <ul class="pagination">
	           		#set($pageRequestURL="$!{www}/sysUser?pageIndex=[PAGINATION_PAGE_INDEX]#if($!{result.queryParameters})&$!{result.queryParameters}#end") #paginationHelper($result $pageRequestURL)
	            </ul>
            </div>
    </div><!-- /.page-content -->
</div><!-- /.main-content -->