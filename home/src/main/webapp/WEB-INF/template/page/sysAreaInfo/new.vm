#set($layout = "/layout/default2.htm")
#set($pageTitle = "title")
#set($pageFootCode = '<script>seajs.use("page/SysAreaInfo/new-update")</script>')
<div class="main-content">
    <div class="page-content">
        <form action="${www}/sysAreaInfo/save"  method="post" class="form-horizontal" id="inputForm_sysAreaInfo">
            <table class="alt-table">
	                 <tr>
	                    <td class="table-tit">id主键</td>
	                    <td>
                        	<input type="text" class="col-sm-10" id="id" name="id" value="$!{param.id}" />
                        </td>
	                 </tr>      
	                 <tr>
	                    <td class="table-tit">区域code</td>
	                    <td>
                        	<input type="text" class="col-sm-10" id="code" name="code" value="$!{param.code}" />
                        </td>
	                 </tr>      
	                 <tr>
	                    <td class="table-tit">区域名</td>
	                    <td>
                        	<input type="text" class="col-sm-10" id="areaname" name="areaname" value="$!{param.areaname}" />
                        </td>
	                 </tr>      
	                 <tr>
	                    <td class="table-tit">排序</td>
	                    <td>
                        	<input type="text" class="col-sm-10" id="sort" name="sort" value="$!{param.sort}" />
                        </td>
	                 </tr>      
	                 <tr>
	                    <td class="table-tit">是否有效1有效2无效</td>
	                    <td>
                        	<input type="text" class="col-sm-10" id="vaild" name="vaild" value="$!{param.vaild}" />
                        </td>
	                 </tr>      
	                 <tr>
	                    <td class="table-tit">父区域code</td>
	                    <td>
                        	<input type="text" class="col-sm-10" id="parentcode" name="parentcode" value="$!{param.parentcode}" />
                        </td>
	                 </tr>      
           </table>
           <div class="fix-actions">
           		<button class="btn btn-cancel" type="button">取消</button>
           		<button class="btn btn-submit" type="submit">提交</button>
           </div>
        </form>
    </div><!-- /.page-content -->
</div><!-- /.main-content -->