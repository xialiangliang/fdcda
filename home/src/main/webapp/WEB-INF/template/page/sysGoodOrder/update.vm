#set($layout = "/layout/layout2.vm")
#set($pageTitle = "title")
#set($JS = ["/js/jquery.form.min.js", "/js/page/sysGoodOrder/new-update.js"])
<div class="col-sm-12">
    <form action="/sysGoodOrder/update"  method="post" class="form-horizontal" id="updateForm_sysGoodOrder">
        <input type="hidden" name="id" value="$!{param.id}" />
        <div class="form-group">
            <label for="id" class="col-sm-2 control-label">主键id</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="id" name="id" value="$!{param.id}" >
            </div>
        </div>    
        <div class="form-group">
            <label for="userId" class="col-sm-2 control-label">用户id</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="userId" name="userId" value="$!{param.userId}" >
            </div>
        </div>    
        <div class="form-group">
            <label for="customerId" class="col-sm-2 control-label">采购商id</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="customerId" name="customerId" value="$!{param.customerId}" >
            </div>
        </div>    
        <div class="form-group">
            <label for="state" class="col-sm-2 control-label">订单状态</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="state" name="state" value="$!{param.state}" >
            </div>
        </div>    
        <div class="form-group">
            <label for="memo" class="col-sm-2 control-label">备注</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="memo" name="memo" value="$!{param.memo}" >
            </div>
        </div>    
        <div class="form-group">
            <label for="createTime" class="col-sm-2 control-label">创建时间</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="createTime" name="createTime" value="$!{param.createTime}" >
            </div>
        </div>    
        <div class="form-group">
            <label for="modifyTime" class="col-sm-2 control-label">修改时间</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="modifyTime" name="modifyTime" value="$!{param.modifyTime}" >
            </div>
        </div>    
        <div class="form-group" align="right">
            <div class="col-sm-12">
                <button type="button" class="btn btn-default btn-close">关闭</button>
                <button type="submit" class="btn btn-primary">提交</button>
            </div>
        </div>
    </form>
</div>