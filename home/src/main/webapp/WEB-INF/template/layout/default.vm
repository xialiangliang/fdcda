<body class="main_bg">
    <div class="header">
        #parse("/widget/header.vm")
    </div>
    <div class="container-fluid">
        #if(${user.id})
        #parse("/widget/frame.vm")
        <div class="main col-sm-10">
            $screen_content
        </div>
        #else
        <style type="text/css">
        .main_bg {
            background: url(/upload/img/bk.jpg) center top no-repeat;
            background-attachment: fixed;
            position: relative;
        }
        </style>
        <div>
            $screen_content
        </div>
